bool isPalindrome(string &str){

int i=0;

int j = str.size()-1;

while(i<j){

if(str[i++] != str[j--]){

return false;

}

}

return true;

}

bool checkPalindrome(string s)

{

// Write your code here.

string str = "";

for (int i = 0; i < s.size(); i++) {

char ch = s[i];

if ((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z') ||
    (ch - 48 >= 0 && ch - 48 <= 9)) {

str += s[i];
}
}
}
